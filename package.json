{
	"name": "minifiers",
	"version": "8.0.0",
	"description": "HTTP minification proxy for HTML, CSS, JS, and SVG.",
	"main": "server.js",
	"engines": {
		"node": ">=16.19.0"
	},
	"dependencies": {
		"@swc/core": "^1.3.34",
		"csso": "^5.0.5",
		"fastify": "^4.12.0",
		"html-minifier": "^4.0.0",
		"jsonminify": "^0.4.2",
		"mime-types": "^2.1.35",
		"node-getopt": "git://github.com/tuxpoldo/node-getopt.git#05e498731c14b648fa332ca78d3a301c5e4be440",
		"superagent": "^8.0.9",
		"svgo": "^3.0.2"
	},
	"devDependencies": {
		"jest": "^29.4.2",
		"prettier": "npm:wp-prettier@^2.8.5",
		"supertest": "^6.3.3"
	},
	"scripts": {
		"prettify": "prettier --write \"lib/**/*\" \"routes/**/*\" \"tests/**/*.test.js\" \"server.js\" \"config.js\"",
		"test": "node --dns-result-order=ipv4first ./node_modules/jest/bin/jest.js ./tests"
	}
}
